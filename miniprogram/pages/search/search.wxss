.table {
  border: 0px solid darkgray;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
 }
 .td {
 
  flex:1;
  justify-content: center;
  text-align: center;
 }
 .bg-w{
  background: snow;
 }
 .bg-g{
  background: #E6F3F9;
 }
 .th {
  flex:1;
  justify-content: center;
  background: #39a8f7;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  font-size:27rpx;
 }

 .st-detail {
   width: 100%;
   height: 100%;
   background-color: #4CAF50;
    padding: 10rpx;
    border-radius: 10rpx;
 }

 .top{
  background: #f7f5ed;
  height: 100rpx;
 
  text-align: center;
  vertical-align: middle;
  line-height: 90rpx;  
 }

 .top_icon{
   float: left;
   font-size:50rpx;
   color:#0e0e0f;
   margin-left: 20rpx;

 }
